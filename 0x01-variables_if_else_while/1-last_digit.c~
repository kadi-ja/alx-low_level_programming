#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
*main - random numbers
*last - the last digit
*Return: 0 success
*/
int main(void)
{
int n;
int l;
char str[] = "Last digit of";
srand(time(0));
n = rand() - RAND_MAX / 2;
l = n % 10;
if (l > 5)
printf("%s %d is %d and is greater than 5\n", n, l);
else if (l == 0)
{
printf("%s %d is %d and is 0\n", n, l);
}
else if (l < 6)
{
printf("%s %d is %d and is less than 6and not 0\n", n, l);
}
return (0);
}
